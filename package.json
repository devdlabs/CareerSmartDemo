{
  "name": "career-smarts",
  "version": "0.0.1",
  "description": "Idea for CareerSmarts website",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf build/",
    "start": "babel-node server.js",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ignore-styles --require ./test/test_helper.js",
    "test:watch": "npm run test -- --watch",
    "postinstall": "webpack -p --config ./webpack.production.config.js --progress --colors",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p --config ./webpack.production.config.js --progress --color",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint app/",
    "commit": "git-cz"
  },
  "author": "Sean Campbell <seancampbell13@gmail.com> (http://seancampbellnatac.com/)",
  "license": "MIT",
