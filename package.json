    "babel-preset-stage-0": "6.3.13",
    "bluebird": "3.2.2",
    "express": "4.13.3",
    "history": "1.17.0",
    "immutable": "3.7.6",
    "material-ui": "0.14.4",
    "ramda": "0.18.0",
    "react": "0.14.7",
    "react-dom": "0.14.3",
    "react-fa": "4.0.0",
    "react-immutable-proptypes": "1.5.0",
